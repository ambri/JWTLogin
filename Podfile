# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'JWTLogin' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for JWTLogin
  pod 'Swinject'
  pod 'SwinjectStoryboard'
  pod 'RxSwift', '~> 4.0'
  pod 'RxSwiftExt', '~> 3.3'
  pod 'RxCocoa', '~> 4.0'
  pod 'Moya/RxSwift'
  pod 'JWTDecode', '~> 2.2'
  pod 'RxViewController'

  target 'JWTLoginTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Nimble', '~> 7.3'
    pod 'Quick', '~> 1.3'
    pod 'RxBlocking', '~> 4.0'
  end

end
